	pages = {699--722},
	annote = {Publisher: [Canadian Political Science Association, Cambridge University Press, Société québécoise de science politique]},
	file = {JSTOR Full Text PDF:C\:\\Users\\noaho\\Zotero\\storage\\TB5773ZX\\Breux et al. - 2017 - Turnout in Local Elections Evidence from Canadian.pdf:application/pdf},
}

