@misc{greifer_matching_2024,
	title = {Matching {Methods}},
	url = {https://cran.r-project.org/web/packages/MatchIt/vignettes/matching-methods.html#specifying-the-propensity-score-or-other-distance-measure-distance},
	abstract = {MatchIt implements several matching methods with a variety of options. Though the help pages for the individual methods describes each method and how they can be used, this vignette provides a broad overview of the available matching methods and their associated options. The choice of matching method depends on the goals of the analysis (e.g., the estimand, whether low bias or high precision is important) and the unique qualities of each dataset to be analyzed, so there is no single optimal choice for any given analysis. A benefit of nonparametric preprocessing through matching is that a number of matching methods can be tried and their quality assessed without consulting the outcome, reducing the possibility of capitalizing on chance while allowing for the benefits of an exploratory analysis in the design phase (Ho et al. 2007).},
	urldate = {2024-12-11},
	author = {Greifer, Noah},
	month = nov,
	year = {2024},
}

